                  <items count="12">
                    <item name="CodeInput" type_name="gh_string" type_code="10">import Rhino.Geometry as rg
import math
if r is None:
    r = rg.Plane(rg.Point3d(0,0,0), rg.Vector3d.XAxis, rg.Vector3d.YAxis)

angle = rg.Vector3d.VectorAngle(p.XAxis, r.XAxis)
if angle &gt;= math.pi / 2:
    vx = -p.XAxis
    vy = -p.YAxis
    np = rg.Plane(p.Origin, vx, vy)
else:
    np = p</item>
